type Product @key(fields: "id") {
    id: ID!
    name: String!
    description: String
    price: Float!
    stockQuantity: Int!
}

input ProductInput {
    name: String!
    description: String
    price: Float!
    stockQuantity: Int!
}

input ProductUpdateInput {
    name: String
    description: String
    price: Float
    stockQuantity: Int
}